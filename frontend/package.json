    "vue-axios": "^3.5.2",
    "vue-cookies": "^1.8.3",
    "vue-flatpickr-component": "^8.1.1",
    "vue-lazyload": "^1.2.6",
    "vue-router": "^3.0.2",
    "vue2-transitions": "^0.2.3"
